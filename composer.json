{
    "name": "konceiver/laravel-rewards",
    "description": ":package_description",
    "keywords": [
        "konceiver",
        "laravel-rewards"
    ],
    "homepage": "https://github.com/konceiver/laravel-rewards",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@konceiver.dev",
            "homepage": "https://konceiver.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/support": "^8.0",
        "spatie/laravel-package-tools": "^1.9"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "graham-campbell/analyzer": "^3.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5",
        "spatie/phpunit-snapshot-assertions": "^4.2",
        "symfony/var-dumper": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "Konceiver\\Skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Konceiver\\Skeleton\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html=.coverage --coverage-clover=clover.xml",
        "format": "vendor/bin/php-cs-fixer fix ./"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Konceiver\\Skeleton\\SkeletonServiceProvider"
            ],
            "aliases": {
                "Skeleton": "Konceiver\\Skeleton\\SkeletonFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
